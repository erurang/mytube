  "scripts": {
    "dev:server": "nodemon babel-node gggg/init.js --delay 2 --ignore '.scss' --ignore 'static'",
    "dev:assets": "cd gggg && WEBPACK_ENV=development webpack -w",
    "build:assets": "cd gggg && WEBPACK_ENV=production webpack",
    "build:server": "babel gggg --out-dir build --ignore 'gggg/assets','gggg/static','gggg/webpack.config.js'",
    "build": "npm run build:server && npm run build:assets && cp -R gggg/static build && cp -R  gggg/views build",
    "prebuild" : "rm -rf build",
    "start" : "PRODUCTION=true node build/init.js",
    "prestart" : "npm run build",
    "tunnel" : "lt --port 4000"
  },
