//- 검색후 비디오가 검색되는 화면
extends ./layouts/main.pug

include ./mixins/videoBlock.pug

block content
    span #{search} 검색결과
    .videos
        each video in videos
            +videoBlock({
                title:video.title,
                views:video.views,
                fileUrl:video.fileUrl,
                id:video.id
            })
        