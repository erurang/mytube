//- 메인 홈화면 올라온 비디오가 순서대로 올라가있을 계획
extends ./layouts/main.pug
include ./mixins/videoBlock.pug


block content
    span#jshome
    .videoWrap
        each video in videos
            +videoBlock({
                fileUrl:video.fileUrl,
                title:video.title,
                avata:video.creator.avataUrl,
                creator:video.creator.name,
                views:video.views,
                createdAt:video.createdAt,
                id:video.id
            })